{
  "name": "Vyelp",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "I bet do you want to see videos in Yelp's page! This it for you. It'll allow you to videos direct from Yelp's page",
  "default_locale": "en",
  "permissions": [
    "https://www.yelp.com/biz/*"
  ],
  "web_accessible_resources": [
    "css/inject.css"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://www.yelp.com/biz/*"
      ],
      "css": [
        "css/inject.css"
      ]
    },
    {
      "matches": [
        "https://www.yelp.com/biz/*"
      ],
      "js": [
        "js/jquery.min.js",
        "dist/inject.js"
      ]
    }
  ]
}
